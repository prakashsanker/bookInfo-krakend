{
    "endpoint_group": [
      {
        "endpoint": "/bookinfo/user/bookuser/{id}",
        "backend": "/user/{id}",
        "method": "GET",
        "extra_config": {
          "krakend-ce/gwproxy/token": {
            "two_fa_exempt": true,
            "two_fa_hard_check": false,
            "onboarding_validate": false,
            "one_fa_hard_check": true
          },
          "krakend-ce/gwproxy/rate-limit": {
